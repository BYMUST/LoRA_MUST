{
    "prefix": "reproduce",
    "dataset": "cifar224",
    "memory_size": 0,
    "memory_per_class": 0,
    "fixed_memory": false,
    "shuffle": true,
    "init_cls": 10,
    "increment": 10,
    "model_name": "adam_lora",
    "backbone_type": "pretrained_vit_b16_224_lora",
    "device": ["0"],
    "seed": [1993],

    "tuned_epoch":5,
    "init_lr":0.01,
    "batch_size":48,
    "weight_decay":0.001,
    "min_lr":0,

    "optimizer":"sgd",
    "vpt_type":"shallow",
    "prompt_token_num":5,

    "lora_dim": 256,
    "lora_scaling": 32,
    "lora_droppout": 0.1,
    "lora_names":["q_proj","v_proj","k_proj","fc1","fc2"]
}

